print "what state are you from?"
input state$
print "are you single, married, separated, or divorced?"
input marital$
Select case state$

    case "CA", "NV", "WA"
        westernregion=westernregion+1
        select case marital$
            case "married"
                westernmarriedcount=westernmarriedcount+1
            case "single"
                westernsinglecount=westernsinglecount+1
            case "separated"
                westernseparatedcount=westernseparatedcount+1
            case "divorced"
                westerndivorcedcount=westerndivorcedcount+1
            case else
                print "unknown marital status!"
        end select
    case "NY", "MA", "FL"
        easternregion=easternregion+1
        select case marital$
            case "married"
                easternmarriedcount=easternmarriedcount+1
            case "single"
                easternsinglecount=easternsinglecount+1
            case "separated"
                easternseparatedcount=easternseparatedcount+1
            case "divorced"
                easterndivorcedcount=easterndivorcedcount+1
            case else
                print "you don't exist in the eastern region!"
        end select
    case "TX", "AL", "GA"
        southernregion=southernregion+1
        select case marital$
            case "married"
                southernmarriedcount=southernmarriedcount+1
            case "single"
                southernsinglecount=southernsinglecount+1
            case "separated"
                southernseparatedcount=southernseparatedcount+1
            case "divorced"
                southerndivorcedcount=southerndivorcedcount+1
            case else
                print "unknown marital status!"
        end select
    case else
        print "You must be from the Midwestern states"
end select

